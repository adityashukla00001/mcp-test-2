{
  "routes": [
    {
      "route": "*.svg",
      "serve": "assets/*.svg",
      "statusCode": 200
    },
    {
      "route": "/assets/*",
      "serve": "/assets/*",
      "statusCode": 200
    },
    {
      "route": "*.css",
      "serve": "*.css",
      "statusCode": 200,
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "route": "*.js",
      "serve": "*.js",
      "statusCode": 200,
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "*.json",
      "serve": "*.json",
      "statusCode": 200,
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "route": "*.md",
      "serve": "*.md",
      "statusCode": 200,
      "headers": {
        "Content-Type": "text/markdown"
      }
    },
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{css,svg,png,gif,ico,jpg,jpeg}"]
  },
  "mimeTypes": {
    ".svg": "image/svg+xml",
    ".woff": "font/woff",
    ".woff2": "font/woff2",
    ".eot": "application/vnd.ms-fontobject",
    ".ttf": "font/ttf",
    ".otf": "font/otf"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=3600"
  },
  "auth": {
    "identityProviders": {
      "github": {
        "registration": {
          "openIdIssuer": "https://token.actions.githubusercontent.com",
          "clientIdSettingName": "GITHUB_CLIENT_ID",
          "clientSecretSettingName": "GITHUB_CLIENT_SECRET"
        }
      }
    }
  }
}
